{
    "redirects": [
      {
        "source": "/(.*)",
        "has": [ { "type": "host", "value": "www.mattevents.fr" } ],
        "destination": "https://mattevents.fr/$1",
        "permanent": true
      }
    ],
  "rewrites": [
    { "source": "/(robots.txt|sitemap.xml)", "destination": "/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|woff2|png|jpg|svg)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
